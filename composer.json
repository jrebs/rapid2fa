{
    "name": "jrebs/rapid2fa",
    "description": "Seamlessly add Google2FA support to Laravel Auth",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Rebelo",
            "email": "justin.rebelo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "pragmarx/google2fa-laravel": "^1.0",
        "laravel/framework": ">=6",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Jrebs\\Rapid2FA\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jrebs\\Rapid2FA\\Providers\\Rapid2FAServiceProvider"
            ]
        }
    }
}
